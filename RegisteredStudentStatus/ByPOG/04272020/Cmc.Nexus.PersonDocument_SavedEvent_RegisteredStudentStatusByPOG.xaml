    <Flowchart.StartNode>
      <FlowStep x:Name="__ReferenceID2" sap2010:WorkflowViewState.IdRef="FlowStep_5">
        <Sequence DisplayName="Init" sap2010:WorkflowViewState.IdRef="Sequence_2">
